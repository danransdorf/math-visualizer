{
  "name": "math-visualizer",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "node scripts/dev_with_watch.js",
    "dev:plain": "vite",
    "build": "npm run build:data && vite build",
    "preview": "vite preview",
    "render:manim": "python3 scripts/render_manim.py",
    "build:proofs": "python3 scripts/render_manim.py",
    "build:proofs:text": "python3 scripts/render_manim.py --text-only",
    "build:definitions": "python3 scripts/build_definitions.py --render-animations",
    "build:data": "npm run build:proofs && npm run build:definitions",
    "build:text": "npm run build:proofs:text && python3 scripts/build_definitions.py"
  },
  "devDependencies": {
    "vite": "^5.2.8"
  },
  "author": "",
  "license": "ISC",
  "description": "Plain JS web component single-page app powered by Vite",
  "dependencies": {
    "katex": "0.16.27"
  }
}
